#include <stdio.h>

int
main ()
{
  int i;

  int j = 1;

  int z = 1;



  for (int i = 1; i <= 100; i = i + 1)
    {

      printf ("%d \n", i);

    }



  while (j <= 100)
    {

      printf ("%d \n", j);

      j = j + 1;
    }
  do
    {

      printf ("%d \n", z);

      z = z + 1;

    }
  while (z <= 100);
  return 0;
}
